<div class="main-content">
  <div class="section__content section__content--p30">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <div class="overview-wrap">
            <h2 class="title-1">Resultados</h2>
          </div>
        </div>
      </div>
      <% @forms.each do |form| %>
        <div class="row m-t-25">
          <div class="col-sm-6 col-lg-3">
            <div class="overview-item overview-item--sform">
              <div class="overview__inner">
                <div class="overview-box clearfix">
                  <%= link_to form.name, form_summary_path(form) %>
                  <div class="text">
                    <div class="icons-container">
                      <%= link_to results_path(export: :graph, form_id: form.id )  do%>
                        <i class="fas fa-chart-pie"></i>
                      <% end %>
                      <%= link_to results_path(export: :csv,form_id: form.id)  do%>
                        <i class="fas fa-download"></i>
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      <% end %>
      <div>
      </div>
    </div>
  </div>
</div>